{
  "name": "@shaharmor/browser-module-boilerplate",
  "version": "0.0.0",
  "description": "browser-module-boilerplate",
  "keywords": [],
  "homepage": "https://github.com/shaharmor/browser-module-boilerplate",
  "bugs": {
    "url": "https://github.com/shaharmor/browser-module-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaharmor/browser-module-boilerplate.git"
  },
  "license": "MIT",
  "author": {
    "name": "Shahar Mor"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/entry.js",
  "types": "dist/entry.d.ts",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf ./coverage ./dist",
    "dev": "NODE_ENV=development webpack-dev-server",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:all": "npm run all -- run lint",
    "lint:eslint": "npm run lint:eslint:glob -- '**/*.{js,ts}'",
    "lint:eslint:glob": "eslint --max-warnings 0",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "prepublish": "npm run build",
    "reformat": "npm run reformat:glob -- '**/*.{js,json,ts}'",
    "reformat:glob": "prettier --write",
    "test": "NODE_ENV=development jest",
    "test:watch": "npm run test -- '--watch'"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@shaharmor/commitlint-config": "0.0.2",
    "@shaharmor/eslint-config": "1.2.7",
    "@shaharmor/prettier-config": "1.2.0",
    "@shaharmor/tsconfig": "0.1.2",
    "@types/expect-puppeteer": "4.4.0",
    "@types/jest": "24.9.0",
    "@types/jest-environment-puppeteer": "4.3.1",
    "@types/node": "13.1.0",
    "@types/puppeteer": "2.0.0",
    "@types/webpack": "4.41.0",
    "@types/webpack-dev-server": "3.9.0",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "eslint": "6.8.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-puppeteer": "4.4.0",
    "lint-staged": "9.5.0",
    "merge-options": "2.0.0",
    "prettier": "1.19.1",
    "puppeteer": "2.0.0",
    "sort-package-json": "1.31.0",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
