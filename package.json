{
  "name": "@shaharmor/browser-module-boilerplate",
  "version": "0.0.0",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf ./coverage ./dist",
    "dev": "webpack-dev-server",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:all": "npm run all -- run lint",
    "lint:eslint": "npm run lint:eslint:glob -- '**/*.{js,ts}'",
    "lint:eslint:glob": "eslint --max-warnings 0",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "prepublish": "npm run build",
    "reformat": "npm run reformat:glob -- '**/*.{js,json,ts}'",
    "reformat:glob": "prettier --write",
    "test": "NODE_ENV=development karma start --single-run",
    "test:watch": "npm run test -- '--no-single-run'"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@shaharmor/commitlint-config": "0.0.1",
    "@shaharmor/eslint-config": "1.1.3",
    "@shaharmor/prettier-config": "1.0.1",
    "@shaharmor/tsconfig": "0.0.4",
    "@types/eventemitter3": "2.0.2",
    "@types/expect": "1.20.4",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.5",
    "@types/sinon": "7.5.0",
    "@types/webpack": "4.39.8",
    "@types/webpack-dev-server": "3.4.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "eslint": "6.6.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "expect": "24.9.0",
    "husky": "3.0.9",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.2",
    "lint-staged": "9.4.2",
    "mocha": "6.2.2",
    "prettier": "1.18.2",
    "sinon": "7.5.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  }
}
